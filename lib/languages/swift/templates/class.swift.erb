<%= autogenerated_warning(@current_class) %>

import Foundation

<%= documentation_block(@current_class,"class") %>

class <%= class_name(@current_class) %>:  <%= @prefix+superclass(@current_class) %><%= mapper_protocol(meta(:mapper),@current_class) %> {

<%= 
    (@current_class[:field].map do |f|
        field_variable(f,@current_class)
    end).join("\n")
%>

<% if meta(:mapper) then %>
<%= mapper_block(meta(:mapper),@current_class) %>
<% end %>
}
